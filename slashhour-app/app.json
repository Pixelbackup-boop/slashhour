{
  "expo": {
    "name": "slashhour-app",
    "slug": "slashhour-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "slashhour",
    "notification": {
      "color": "#E63946",
      "androidMode": "default"
    },
    "intentFilters": [
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": [
          {
            "scheme": "https",
            "host": "slashhour.com",
            "pathPrefix": "/deals"
          },
          {
            "scheme": "https",
            "host": "www.slashhour.com",
            "pathPrefix": "/deals"
          }
        ],
        "category": ["BROWSABLE", "DEFAULT"]
      }
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#E63946",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Slashhour to access your camera to scan QR codes for redemptions.",
          "microphonePermission": false
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.slashhourapp",
      "associatedDomains": [
        "applinks:slashhour.com",
        "applinks:www.slashhour.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.slashhourapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7afef91d-4867-4fd0-9aea-151b1f16bc42"
      }
    },
    "owner": "slashhour"
  }
}
