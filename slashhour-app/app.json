{
  "expo": {
    "name": "slashhour-app",
    "slug": "slashhour-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "slashhour",
    "notification": {
      "color": "#E63946",
      "androidMode": "default"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#E63946",
          "defaultChannel": "default"
        }
      ]
    ],
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "slashhour"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.slashhourapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.slashhourapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7afef91d-4867-4fd0-9aea-151b1f16bc42"
      }
    },
    "owner": "slashhour"
  }
}
